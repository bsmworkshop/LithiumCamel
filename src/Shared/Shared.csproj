    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>LithiumCamel.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>LithiumCamel.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
